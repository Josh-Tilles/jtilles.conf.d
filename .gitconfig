[user]
	name = Josh Tilles
	email = MerelyAPseudonym@gmail.com
[core]
	excludesfile = /Users/joshua.tilles/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[alias]
	unstage = reset HEAD --
	gitl = log --pretty=oneline --graph --decorate --all
[push]
	default = upstream
[color]
	ui = auto
